<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wizard.fems.enMonit.alarm.alarmMapper">

    <select id="getCustom" resultType="hashmap">
        select  customID
             ,  kCustom
        from mt_custom
        where businessTypeCode = '02'
    </select>

    <select id="getMachine" resultType="hashmap">
        select  mcID
             ,  mcName
        from mt_mc

    </select>


</mapper>